{
  "name": "typescript-api",
  "version": "1.0.0",
  "description": "Api Com TypeScript",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:dev": "ts-node-dev src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test": "echo \"Error: no test specified\" && exit 1",
    "test:functional": "jest --projects ./test --runInBand",
    "style:check": "prettier --check src/**/*.ts test/**/*.ts",
    "style:fix": "prettier --fix src/**/*.ts test/**/*.ts"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/PedroHaratani/ApiNodeJs.git"
  },
  "keywords": [
    "nodejs",
    "typescript",
    "api"
  ],
  "author": "Pedro Shunzo",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/PedroHaratani/ApiNodeJs/issues"
  },
  "homepage": "https://github.com/PedroHaratani/ApiNodeJs#readme",
  "devDependencies": {
    "@types/babel-core": "^6.25.7",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.0",
    "@types/node": "^18.15.3",
    "@types/supertest": "^2.0.12",
    "@typescript-eslint/eslint-plugin": "^5.55.0",
    "@typescript-eslint/parser": "^5.55.0",
    "eslint": "^8.36.0",
    "jest": "^29.5.0",
    "prettier": "^2.8.4",
    "supertest": "^6.3.3",
    "ts-jest": "^29.0.5",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.0.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.6",
    "@types/module-alias": "^2.0.1",
    "body-parser": "^1.20.2",
    "express": "^4.18.2",
    "module-alias": "^2.2.2"
  }
}
